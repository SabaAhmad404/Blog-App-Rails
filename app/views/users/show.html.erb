<%= render 'shared/post', user: @user %>

  <div class="bio"> 
     <h1>Bio</h1>
    <p><%= @user.Bio%></p>
  </div>
  <%@user.most_recent_posts.each do |post|%>
   <%= render 'shared/post', post: post%>
  <%end%>

  <div class="button">
  <%= link_to posts_path(@user) do%>
  <button>see all posts</button>
  <% end%>
  </div>